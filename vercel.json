{
    "version": 2,
    "builds": [
        {
            "src": "api/index.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.12"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/race/(.*)",
            "destination": "/api/index.py"
        }
    ]
}